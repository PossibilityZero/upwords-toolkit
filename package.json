{
  "name": "upwords-toolkit",
  "version": "0.1.21",
  "description": "Collection of tools for the Upwords board game",
  "main": "dist/src/index.js",
  "types": "dist/types/src/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "test": "jest -o",
    "test-watch": "jest --watchAll",
    "test-full": "jest --verbose --coverage",
    "test-precommit": "jest --coverage",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "prettier-format": "prettier --config .prettierrc 'src/**/*' --write",
    "prettier-check": "prettier --config .prettierrc 'src/**/*' --check",
    "prepare": "husky install",
    "generate-docs": "typedoc --out docs src/index.ts --plugin typedoc-plugin-markdown"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PossibilityZero/upwords-toolkit.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PossibilityZero/upwords-toolkit/issues"
  },
  "homepage": "https://github.com/PossibilityZero/upwords-toolkit#readme",
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "typedoc": "^0.26.6",
    "typedoc-plugin-markdown": "^4.2.6",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@kamilmielnik/trie": "^3.0.0"
  }
}
